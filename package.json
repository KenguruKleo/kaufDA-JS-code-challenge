    "concurrently": "^3.4.0",
    "css-loader": "0.19.0",
    "enzyme": "^2.8.2",
    "file-loader": "0.8.5",
    "http-proxy": "^1.16.2",
    "jest": "^20.0.4",
    "json-server": "^0.10.1",
    "nodemon": "^1.11.0",
    "react-addons-test-utils": "15.4.2",
    "react-dom": "15.4.2",
