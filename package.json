  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "start-dev": "concurrently --kill-others \"npm run start-json-server\" \"npm run webpack-dev-server\"",
    "start": "concurrently --kill-others \"npm run start-json-server\" \"npm run server\"",
    "start-json-server": "node ./server/db/json-server/json-server --db_port 3000",
    "webpack-dev-server": "webpack-dev-server --progress --hot --inline --port 8080",
    "build": "webpack -p",
    "server": "node ./server/server --port 8080",
    "test": "jest",
    "codecov": "codecov",
    "test:watch": "npm test -- --watch",
    "mongo": "docker run --name mongo -p 127.0.0.1:27017:27017 -d mongo"
  },
  "author": "Konstantin Yemelianov",
  "license": "ISC",
  "dependencies": {
    "babel-plugin-syntax-object-rest-spread": "6.13.0",
    "bluebird": "3.4.7",
