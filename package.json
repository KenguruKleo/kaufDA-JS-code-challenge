    "react": "15.4.2",
    "react-bootstrap": "^0.31.0",
    "react-dom": "15.4.2",
    "react-redux": "^5.0.5",
    "redux": "^3.6.0",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.2.0"
  },
  "devDependencies": {
    "babel-cli": "6.23.0",
    "babel-core": "6.23.0",
    "babel-jest": "^20.0.3",
    "babel-loader": "6.3.2",
    "babel-polyfill": "6.23.0",
    "babel-preset-es2015": "6.22.0",
